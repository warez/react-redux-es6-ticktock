{
  "name": "angular2-quickstart-es6-webpack",
  "version": "1.0.0",
  "description": "The Angular 2 Quickstart tutorial done in ES6 with webpack and Babel, without using TypeScript.",
  "scripts": {
    "start": "node server.js",
    "start debug": "node $NODE_DEBUG_OPTION server.js",
    "webpack": "webpack --progress",
    "webpack-dev-server": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --progress --colors",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js  \"test/**/*@(.js|.jsx)\" ",
    "test:watch": "npm run test -- --watch",
    "build": "babel ttserver -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varunmulloli/angular2-quickstart-es6-webpack.git"
  },
  "author": "Varun Mulloli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/varunmulloli/angular2-quickstart-es6-webpack/issues"
  },
  "homepage": "https://github.com/varunmulloli/angular2-quickstart-es6-webpack#readme",
  "dependencies": {
    "babel-register": "^6.23.0",
    "core-js": "^2.4.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "flux": "^3.1.2",
    "immutable": "^3.8.1",
    "react": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "systemjs": "0.19.40",
    "ttflux": "file:public/src/ttflux",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.22.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "concurrently": "^2.0.0",
    "css-loader": "^0.26.1",
    "jsdom": "^9.11.0",
    "lite-server": "^2.1.0",
    "mocha": "^3.2.0",
    "react-hot-loader": "^1.3.1",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
